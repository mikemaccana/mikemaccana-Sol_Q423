{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2020"],
    "module": "nodeNext",
    "moduleResolution": "nodeNext",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
